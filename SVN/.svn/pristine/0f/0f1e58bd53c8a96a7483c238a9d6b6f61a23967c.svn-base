<extend name="../welfare/Home/View/Tpl/base.html" />

<block name="title">
  <title>报名页</title>
</block>

<block name="css">
  <link href="{$static}/css/template.css" rel="stylesheet" type="text/css" media="screen">
</block>

<block name="menu">
  <div class="btn-group-vertical levelTwo">
    <a href="#" class="btn btn-info" role="button">活动名</a>
    <a href="#" class="btn btn-info" role="button">查看反馈</a>
    <a href="#" class="btn btn-info" role="button">报名参加</a>
  </div>
</block>

<block name="main">
  <div class="container holder">
    <div class="jumbotron">
      <div class="row">
      <!-- 左侧-->
        <div class="col-sm-3 col-md-2 sidebar well">

        </div>

        <!-- 右侧-->
        <div class="well col-sm-9" role="main">
          <div>
            <form role="form" class="">
              <div class="form-group">
                <label for="exampleInputEmail1">Email address</label>
                <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email">
              </div>
              <button type="submit" class="btn btn-default">提交报名表</button>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</block>
    
<block name="js">
  <script src="{$static}/js/template.js" type="text/javascript"></script>
</block>